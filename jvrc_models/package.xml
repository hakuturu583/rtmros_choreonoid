<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">

  <name>jvrc_models</name>
  <description>
    package for jvrc robot/environment models
  </description>
  <version>0.0.0</version>
  <author>Team NEDO-JSK</author>
  <maintainer email="yohei@jsk.imi.i.u-tokyo.ac.jp">Yohei Kakiuchi</maintainer>
  <license>EPL</license>
  <!--url>http://ros.org/wiki/jsk_models</url-->

  <buildtool_depend condition="$ROS_VERSION == 1">catkin</buildtool_depend>
  <buildtool_depend condition="$ROS_VERSION == 2">ament_cmake</buildtool_depend>
  <buildtool_depend condition="$ROS_VERSION == 2">ament_cmake_auto</buildtool_depend>

  <export>
    <build_type condition="$ROS_VERSION == 1">catkin</build_type>
    <build_type condition="$ROS_VERSION == 2">ament_cmake</build_type>
  </export>

</package>


